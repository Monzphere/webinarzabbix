{
    "manifest_version": 2.0,
    "id": "location-map",
    "name": "Location Map",
    "version": "2.0",
    "namespace": "LocationMap",
    "description": "Visualização de hosts em mapa geográfico, e um resumo de hosts por localidade",
    "author": "Monzphere",
    "actions": {
        "map.view": {
            "class": "MapView",
            "view": "map.view"
        }
    },
    "assets": {
        "js": ["map-viewer.js" ],
        "css": [
            "map-viewer.css"
        ]
    }
} 